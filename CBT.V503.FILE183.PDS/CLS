//GILBERTK JOB (ACCT#),CLS,
// CLASS=A,MSGCLASS=H,COND=(0,NE)
//HLASM EXEC PGM=ASMA90,PARM=(OBJECT,NODECK,NOESD,NORLD,NOXREF)
*---------------------------------------------------------------------*
*        TSO "CLEAR SCREEN" COMMAND                                   *
*---------------------------------------------------------------------*
CLEARSCR CSECT
CLEARSCR RMODE ANY
         STLINENO LINE=1,CLEAR=YES     TOP OF SCREEN
         BR    14                      GOBACK
         DC    C'GSF UTILITIES - CLS R300'
         END
//SYSLIB   DD DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1   DD UNIT=VIO,SPACE=(CYL,1)
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD UNIT=VIO,SPACE=(TRK,1),DISP=(,PASS),BLKSIZE=3200
//*
//LKED    EXEC PGM=HEWL,PARM=(MAP,RENT)
//SYSLIN   DD DSN=*.HLASM.SYSLIN,DISP=(OLD,PASS)
//SYSLMOD  DD DSN=CBTTAPE.FILE183.LOAD(CLS),DISP=SHR
//SYSPRINT DD SYSOUT=*
