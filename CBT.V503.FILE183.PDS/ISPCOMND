/*    MEMBER TO UPDATE THE ISPF COMMAND TABLE IN STORAGE  */
/*  ------ you should enjoy using this immensely ------   */
/*  ------     Invoke by issuing TSO ISPCMDU     ------   */
/FVE/0/SELECT CMD(%FVE PARM &ZPARM) NEWAPPL(ISR)/Fast-ViewEdit
/FP/0/SELECT PGM(FASTPATH)/FASTPATH
/IQI/0/SELECT PANEL(IQI@PRIM) NEWAPPL(IQI)/Spiffy
/FMN/0/SELECT PANEL(FMNSTASK) NEWAPPL(FMN)/File Manager
/MVSXREF/4/SELECT PANEL(XREFPRIM)/MVSXREF/
/JCLXREF/4/SELECT PANEL(JXRFPRIM)/JCLXREF/
/NPHELP/4/SELECT PANEL(NOTEPADH)/Notepad/
/NPANEL/4/SELECT PANEL(NOTEPAD) NEWAPPL(NOTE)/Notepad/
/NOTEPAD/4/SELECT CMD(%NOTEPAD &ZPARM) NEWAPPL(NOTE)/Notepad/
/PROFSRCH/5/SELECT CMD(%THSPROFS &ZPARM) NEWAPPL(ISR) PASSLIB/Profile Srch/
/XSP/0/SELECT PANEL(PDS@PRIM) NEWAPPL(XSP)/SWITCH APPLID/
/REASM/3/SELECT PGM(REASM) NEWAPPL(REA)/Reasm 4.0/
/FDSPACE/3/SELECT CMD(FDSPACE)/Disk Space and contents/
/SMPE/3/SELECT PGM(GIMSTART) PARM(&ZCMD) NOCHECK/SMP/E Dialogs/
/UMB/0/SELECT CMD(UMSGN)/Browse message member in MESAJ.PDS/
/UMSG/2/SELECT CMD(UMSG)/Browse message member in MESAJ.PDS/
/ISMF/0/SELECT PGM(DGTFMD01)/ISMF ISPF DIALOG/
/STACK/3/SELECT PANEL(ISR@PRIM)/STACK A MAIN ISPF PANEL/
/DITTO/3/SELECT PGM(DITTO) /DITTO-ESA/
/SPACE/0/SELECT PANEL(SSCSPC01) SCRNAME(SPACE)/SPACE TSO command/
/MXI/0/SELECT CMD(MXI)/MXI Invocation/
/RACF/2/SELECT PANEL(ICHP00) NEWAPPL(RACF)/RACF Admin Panels/
/SHZOS/0/SELECT CMD(SHOWZOS &ZPARM) MODE(FSCR)/INVOKE SHOWMVS COMMAND/
/SHOWMVS/5/SELECT CMD(SHOWMVS &ZPARM) MODE(FSCR)/INVOKE SHOWMVS COMMAND/
/SHO9716/4/SELECT CMD(SHO9716 &ZPARM) MODE(FSCR)/INVOKE SHOWMVS COMMAND/
/SDSF/0/SELECT PGM(ISFISP) NEWAPPL(ISF)/INVOKE SDSF UNDER ISPF/
/PRIM/3/SELECT PANEL(ISR@PRIM)/main ispf panel/
/PDE/0/SELECT PANEL(PDS@PRIM)/STARTOOL OR PDS 8.5 MAIN ENTRY PANEL/
/PT1/0/SELECT PANEL(PTOOLS01)/SYSTEM PROGRAMMER TOOLS - 1/
/SX/0/ALIAS SAVE                    /SHORT FORM OF SAVE/
/IM/0/SELECT CMD(IM &ZPARM)/INVOKE IMON/
/FDM/0/SELECT CMD(FDM &ZPARM)/INVOKE STARTOOL/
/PDSE/0/SELECT CMD(PDSE &ZPARM)/INVOKE STARTOOL/
/P8/0/SELECT CMD(PDS86 &ZPARM)/INVOKE PDS 8.6/
/PDS8/4/SELECT CMD(PDS86 &ZPARM)/INVOKE PDS 8.6/
/LPTF/0/SELECT CMD(%LPTF &ZPARM)/INVOKE LIST PTFS FROM SMPPTS/
/PRINTO/0/SELECT CMD(%ISPFPRTO &ZPARM)/ISPF INVOKE PRINTOFF/
/SP0 /0/SELECT PGM(ISPISM) SCRNAME(SETTINGS)/ISPF 0/
/SP1 /0/SELECT PGM(ISRBRO) PARM(ISRBRO01) SCRNAME(VIEW)/ISPF 1/
/SP2 /0/SELECT PGM(ISREDIT) PARM(P,ISREDM01) SCRNAME(EDIT)/ISPF 2/
/SP3 /0/SELECT PANEL(ISRUTIL) SCRNAME(UTIL)/ISPF 3/
/SP4 /0/SELECT PANEL(ISRFPA) SCRNAME(FOREGRND)/ISPF 4/
/SP5 /0/SELECT PGM(ISRJB1) PARM(ISRJPA) SCRNAME(BATCH) NOCHECK/ISPF 5/
/SP6 /0/SELECT PGM(ISRPTC) SCRNAME(CMD)/ISPF 6/
/SP6A/0/SELECT PGM(ISRPTC)             /ISPF 6/
/SP7 /0/SELECT PGM(ISPYXDR) PARM(&ZTAPPLID) SCRNAME(DTEST) NOCHECK/SP7/
/SP8 /0/SELECT PANEL(ISRLPRIM) SCRNAME(LMF)/ISPF8/
/SP9 /0/SELECT PANEL(ISRDIIS) ADDPOP/ISPF 9/
/SP10 /0/SELECT PGM(ISRSCLM) SCRNAME(SCLM) NOCHECK/ISPF 10/
/SP11 /0/SELECT PGM(ISRUDA) PARM(ISRWORK) SCRNAME(WORK)/ISPF 11/
/SP31/0/SELECT PGM(ISRUDA) PARM(ISRUDA1) SCRNAME(LIBUTIL)/ISPF 3.1/
/SP32/0/SELECT PGM(ISRUDA) PARM(ISRUDA2) SCRNAME(DSUTIL)/ISPF 3.2/
/SP33/0/SELECT PGM(ISRUMC) SCRNAME(MCOPY)/ISPF 3.3/
/SP34/0/SELECT PGM(ISRUDL) PARM(ISRUDLP) SCRNAME(DSLIST)/ISPF 3.4/
/SP35/0/SELECT PGM(ISRURS) SCRNAME(RESET)/ISPF 3.5/
/SP36/0/SELECT PGM(ISRUHC) SCRNAME(HARDCOPY)/ISPF 3.6/
/SP37/0/SELECT PANEL(ISPUDL) SCRNAME(DOWNLOAD)/ISPF 3.7/
/SP38/0/SELECT PGM(ISRUOLP) SCRNAME(OUTLIST)/ISPF 3.8/
/SP39/0/SELECT PANEL(ISPUCMA) ADDPOP SCRNAME(CMDTABLE)/ISPF 3.9/
/SP311/0/SELECT PGM(ISRFMT) SCRNAME(FORMAT)/ISPF 3.11/
/SP312/0/SELECT PGM(ISRSSM) SCRNAME(SUPERC)/ISPF 3.12/
/SP313/0/SELECT PGM(ISRSEPRM) SCRNAME(SUPERCE) NOCHECK/ISPF 3.13/
/SP314/0/SELECT PGM(ISRSFM) SCRNAME(SRCHFOR)/ISPF 3.14/
/SP315/0/SELECT PGM(ISRSEPRM) PARM(S4) SCRNAME(SRCHFORE) NOCHECK/3.15/
